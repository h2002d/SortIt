@model List<SortItResearch.Models.UserProfile>
<table id="customers" class="subjectTable">
    <tr>
        <th></th>
        <th></th>
        <th></th>
    </tr>
    @foreach(var user in Model)
    {
        <tr style="cursor:pointer;" onclick="window.location.href='/Teacher/Teacher?userId=@user.Id'">
            <td>@user.Name @user.SurName</td>
            <td>@user.Email</td>
            <td></td>
        </tr>
    }
</table>